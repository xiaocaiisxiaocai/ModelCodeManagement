{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "SqlSugar": "Debug"
    }
  },
  "Security": {
    "RequireHttps": false,
    "RateLimiting": {
      "Enabled": false
    }
  },
  "Performance": {
    "CacheExpiration": 60,
    "QueryTimeout": 60
  },
  "DetailedErrors": true,
  "Development": {
    "EnableSwagger": true,
    "ShowSqlLogs": true,
    "EnableCorsForAll": true
  }
}
